.tmpContainer{
  display: flex;
  flex-direction: column;
  border-top: solid 4rpx #eee;
  border-bottom: solid 4rpx #eee;
  margin: 20rpx 0;
}

.avatar_data{
  padding: 10rpx;
}

.avatar_data image{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.avatar_data text{
  font-size: 30rpx;

}

.company{
  margin-left: 10rpx;
  font-size: 36rpx;
  font-weight: 700;
  margin-left: 10rpx;
}

.contenImg{
  width: 100%;
  height: 460rpx;
}

.content{
  font-size: 32rpx;
  text-indent: 62rpx;
}

.collection_love image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.collection_love text{
  font-size: 29rpx;
  margin-right: 10rpx;
}